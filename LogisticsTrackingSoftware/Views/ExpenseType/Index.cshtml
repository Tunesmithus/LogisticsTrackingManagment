@model LogisticsManagement.Data.ExpenseType
@{
    ViewData["Title"] = "Index";
}


<div class="container">
    <div class="row pt-4">
        <div class="col-6">   
             <h2> Expense List</h2>
        </div>
        <div class="col-6 text-end">   
             <p>
                <a asp-action="Create" class="btn btn-primary"><i class="bi bi-plus-circle"></i> &nbsp;Create New</a>
            </p>
        </div>
    </div>

<table id ="tblData" class="table table-bordered table-striped">
    <thead class="table-primary">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ExpenseCategory)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Description)
            </th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
</table>
 
</div>

@section Scripts{
    <script src="~/js/expenseType.js"></script>
}




